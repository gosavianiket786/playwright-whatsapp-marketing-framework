{
  "name": "playwright-bdd-example",
  "version": "0.1.0",
  "scripts": {
    "test": "set PW_HEADLESS=1 && npx bddgen && npx playwright test --grep",
    "test:wpmarketing": "set PW_HEADLESS=1 && npx bddgen && npx playwright test --grep",
    "test:debug": "set PWDEBUG=1 && npx bddgen && npx playwright test --grep",
    "allure:generate": "allure generate out/allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "lint": "eslint .",
    "doc": "typedoc .",
    "watch:bdd": "nodemon -w node_modules/features -w ./steps -e feature,js,ts --exec \\\"npx bddgen\\\"",
    "watch:pw": "playwright test --ui",
    "watch": "run-p watch:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.9.4",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "playwright-bdd": "^6.6.0",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cucumber/cucumber": "^10.9.0",
    "@playwright/test": "^1.46.0",
    "allure-js-commons": "^3.2.1",
    "allure-playwright": "^3.2.1",
    "csv-parse": "^5.5.6",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "cucumber": "^6.0.7",
    "dotenv": "^16.4.5",
    "playwright-bdd-example": "file:",
    "ts-node": "^10.9.2"
  },
  "prettier": {
    "endOfLine": "lf",
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "typescript"
  }
}
